{
    "env": {
        "browser": true,
        "es2021": true,
        "commonjs": true
    },
    "extends": "eslint:recommended",
    "parser": "@babel/eslint-parser",
    "plugins": [
        "flowtype"
    ],
    "globals": {
        "process": true,
        "$": true,
        "__dirname": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-prototype-builtins": "warn",
        "no-self-assign": "warn",
        "no-unused-vars": "warn",
        "no-useless-escape": "warn"
    },
    "root": true
}
